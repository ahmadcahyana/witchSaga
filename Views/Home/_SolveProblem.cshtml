<div class="col-md-12">
    @if (Model.data != null)
    {
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Year of Death</th>
                    <th>Year of Birth</th>
                    <th>Killed</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.data)
                {
                    <tr>
                        <td>@item.AgeofDeath</td>
                        <td>@item.YearofDeath</td>
                        <td>@item.Killed</td>
                    </tr>
                }
            </tbody>
        </table>
        <p class="float-lg-right">Average : <strong>@Model.average</strong></p>
    }
    else
    {
        <h5>Invalid Input!</h5>
        <p>Return -1</p>
    }
</div>
